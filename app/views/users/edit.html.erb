<h1> Edit User</h1>
<%= form_with(model: [@user], local: true) do |form| %>
    <%#= form_with(model: @comment, local: true) do |form| %>

 	<div class="field">
    <%= form.label :email %><br />
    <%= form.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="actions">
    <%= form.submit "Update" %>
  </div>
<% end %>
